<div class="container-fluid">
		
	{% set userName = userData.name ~ ' ' ~ userData.surname %}
	{% if userData.city == '' %}
		{% set userCity = '---' %}
	{% else %}
		{% set userCity = userData.city %}
	{% endif %}
		
	{% include "Profile/jumbotron1.html.twig" %}
		
	{% if myData == userData %}	
		{% set prefix = '' %}
	{% else %}
		{% set prefix = 'user_' %}
	{% endif %}
	
	{%
		set profileMenu = [
			{'path': 'path_' ~ prefix ~ 'home','name':'Posty'},
			{'path': 'path_' ~ prefix ~ 'about','name':'O mnie'},
			{'path': 'path_' ~ prefix ~ 'photo','name':'ZdjÄ™cia'},
			{'path': 'path_' ~ prefix ~ 'friends','name':'Znajomi'}
		]
	%}
	
	{% if myData == userData %}
		{% set param = {} %}
	{% else %}
		{% set param = {'id':userData.id} %}
	{% endif %}
	
	<div class="navbar navbar-inverse">
		<div class="container">
			<div class="navbar-collapse collapse">
				<ul class="nav navbar-nav">
					{% for menu in profileMenu %}
						<li {{ app.request.get('_route') == menu['path'] ? 'class="active"': ''}}>
							<a href="{{path(menu['path'],param)}}">{{ menu.name }}</a>
						</li>
					{% endfor %}
				</ul>
			</div>
		</div>
	</div>	
	
	{% if app.request.get('_route') == 'path_' ~ prefix ~ 'home' %}
		{% set link = app.request.get('_route') %}
		{% include 'Profile/posts.html.twig' %}
	{% endif %}
</div>